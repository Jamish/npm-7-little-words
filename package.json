{
  "name": "7-little-words",
  "version": "1.0.2",
  "description": "A solver library for solving puzzles from 7LittleWords.com",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format"
  },
  "keywords": [
    "seven little words",
    "7 little words",
    "7littlewords",
    "puzzle solver"
  ],
  "author": "Jamison Schuster",
  "license": "CC0 1.0 Universal",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jamish/npm-7-little-words.git"
  },
  "files": [
    "lib/**/*",
    "resources/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
